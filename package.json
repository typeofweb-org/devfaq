  "scripts": {
    "dev": "rm -f server/*.js && nodemon server/server.ts",
    "build": "echo $NODE_ENV && rm -rf ./.next ./public ./server/*.js ./server/*.map && next build && next export -o public && tsc --project tsconfig.server.json",
    "start": "node app.js",
    "analyze": "cross-env ANALYZE=1 NODE_ENV=production next build",
    "test": "tsc --project tsconfig.json --noEmit && tsc --project tsconfig.server.json --noEmit && tslint -p tsconfig.json && tslint -p tsconfig.server.json"
